<?xml version="1.0" encoding="utf-8" ?>
#!target:spring/src/main/java/{{.global.pkg}}/resources/query/pg/{{.table.Prefix}}/{{.table.Title}}List.xml
<!--
    {{.table.Comment}}  generate time: {{.global.Time}}
    this file is auto generated by tto,
    it's should be copy to yourself query folder!
-->
<ExportItemConfig>
    <ColumnMapping>
    {{range $i,$v := .columns}}{{if ne $i 0}};{{end}}{{$v.Name}}:{{$v.Comment}}{{end}}
    </ColumnMapping>
    <Query>
        <![CDATA[
        SELECT * FROM "{{.table.Name}}"
        ORDER BY id DESC LIMIT {page_size} OFFSET {page_start}
     ]]>
    </Query>
    <Import><![CDATA[]]></Import>
    <Total>
        <![CDATA[
            SELECT COUNT(0) FROM "{{.table.Name}}"
        ]]>
    </Total>
</ExportItemConfig>
